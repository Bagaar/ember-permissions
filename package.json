{
  "name": "@bagaar/ember-permissions",
  "version": "0.1.0",
  "description": "Permission management for Ember applications.",
  "keywords": [
    "bagaar",
    "bgr",
    "ember",
    "ember-addon",
    "permissions"
  ],
  "repository": "https://github.com/Bagaar/ember-permissions",
  "license": "MIT",
  "author": "Bagaar",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build",
    "format:js": "prettier-standard './**/*.js'",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint .",
    "start": "ember serve",
    "test": "ember test",
    "test:all": "ember try:each",
    "release": "standard-version"
  },
  "dependencies": {
    "ember-cli-babel": "^7.1.2"
  },
  "devDependencies": {
    "@bagaar/eslint-config-ember": "^0.5.0",
    "@ember/optional-features": "^0.7.0",
    "broccoli-asset-rev": "^3.0.0",
    "ember-cli": "~3.9.0",
    "ember-cli-dependency-checker": "^3.1.0",
    "ember-cli-eslint": "^5.1.0",
    "ember-cli-htmlbars": "^3.0.0",
    "ember-cli-htmlbars-inline-precompile": "^2.1.0",
    "ember-cli-inject-live-reload": "^2.0.1",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-template-lint": "^1.0.0-beta.1",
    "ember-cli-uglify": "^2.1.0",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.0",
    "ember-load-initializers": "^2.0.0",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-qunit": "^4.2.0",
    "ember-resolver": "^5.0.1",
    "ember-source": "~3.9.0",
    "ember-source-channel-url": "^1.1.0",
    "ember-try": "^1.0.0",
    "eslint-plugin-ember": "^6.3.0",
    "eslint-plugin-node": "^8.0.1",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5",
    "loader.js": "^4.7.0",
    "prettier-standard": "^9.1.1",
    "qunit-dom": "^0.8.0",
    "standard-version": "^4.4.0"
  },
  "engines": {
    "node": "8.* || >= 10.*"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  },
  "lint-staged": {
    "*.js": [
      "prettier-standard",
      "eslint --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
