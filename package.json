{
  "name": "@bagaar/ember-permissions",
  "version": "3.0.0",
  "description": "Permission management for Ember applications.",
  "keywords": [
    "bagaar",
    "ember",
    "ember-addon",
    "permissions"
  ],
  "repository": "https://github.com/Bagaar/ember-permissions",
  "license": "MIT",
  "author": "Bagaar",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel \"lint:!(fix)\"",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel \"lint:*:fix\"",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "lint:types": "tsc --noEmit",
    "prepack": "ember ts:precompile",
    "postpack": "ember ts:clean",
    "prepare": "husky install",
    "release": "standard-version",
    "start": "ember serve",
    "test": "npm-run-all \"lint\" \"test:*\"",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each"
  },
  "dependencies": {
    "@glimmer/tracking": "^1.1.2",
    "ember-cli-babel": "^7.26.11",
    "ember-cli-typescript": "^5.2.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@ember/optional-features": "^2.0.0",
    "@ember/test-helpers": "^2.8.1",
    "@embroider/test-setup": "^1.8.3",
    "@glimmer/component": "^1.1.2",
    "@tsconfig/ember": "^1.0.1",
    "@types/ember": "^4.0.2",
    "@types/ember-qunit": "^5.0.2",
    "@types/ember-resolver": "^5.0.13",
    "@types/ember__application": "^4.0.4",
    "@types/ember__array": "^4.0.3",
    "@types/ember__component": "^4.0.11",
    "@types/ember__controller": "^4.0.3",
    "@types/ember__debug": "^4.0.3",
    "@types/ember__destroyable": "^4.0.1",
    "@types/ember__engine": "^4.0.4",
    "@types/ember__error": "^4.0.1",
    "@types/ember__object": "^4.0.5",
    "@types/ember__polyfills": "^4.0.1",
    "@types/ember__routing": "^4.0.12",
    "@types/ember__runloop": "^4.0.2",
    "@types/ember__service": "^4.0.1",
    "@types/ember__string": "^3.0.10",
    "@types/ember__template": "^4.0.1",
    "@types/ember__test": "^4.0.1",
    "@types/ember__test-helpers": "^2.8.2",
    "@types/ember__utils": "^4.0.2",
    "@types/qunit": "^2.19.3",
    "@types/rsvp": "^4.0.4",
    "@typescript-eslint/eslint-plugin": "^5.44.0",
    "@typescript-eslint/parser": "^5.43.0",
    "broccoli-asset-rev": "^3.0.0",
    "ember-auto-import": "^2.4.3",
    "ember-cli": "~4.8.0",
    "ember-cli-dependency-checker": "^3.3.1",
    "ember-cli-htmlbars": "^6.1.1",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.2",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-load-initializers": "^2.1.2",
    "ember-page-title": "^7.0.0",
    "ember-qunit": "^6.0.0",
    "ember-resolver": "^8.0.3",
    "ember-source": "~4.8.0",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^5.0.1",
    "ember-try": "^2.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-ember": "^11.1.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-qunit": "^7.3.1",
    "husky": "^8.0.2",
    "loader.js": "^4.7.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "qunit": "^2.19.2",
    "qunit-dom": "^2.0.0",
    "standard-version": "^9.5.0",
    "typescript": "^4.9.3",
    "webpack": "^5.74.0"
  },
  "engines": {
    "node": "14.* || 16.* || >= 18"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  },
  "volta": {
    "node": "14.19.3",
    "yarn": "1.22.17"
  }
}
